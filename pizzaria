package senac.helloworld.classes;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.SQLException;
import java.util.logging.Level;
import java.util.logging.Logger;
import senac.helloworld.DataBase;

/**
 *
 * @author Suporte
 */
public class Pedido{
        String tamanho;
        String sabor;
        String nomeCliente;
        String pagamento;
        String localEntr;

        public String getTamanho() {
            return tamanho;
        }
        public void setTamanho(String tamanho) {
            this.tamanho = tamanho;
        }
        
        public String getSabor() {
            return sabor;
        }
        public void setSabor(String sabor) {
            this.sabor = sabor;
        }

        public String getNomeCliente() {
            return nomeCliente;
        }
        public void setNomeCliente(String nomeCliente) {
            this.nomeCliente = nomeCliente;
        }
        
        public String getPagamento() {
            return pagamento;
        }
        public void setPagamento(String pagamento) {
            this.pagamento = pagamento;
        }

        public String getLocalEntr() {
            return localEntr;
        }
        public void setLocalEntr(String localEntr) {
            this.localEntr = localEntr;
        }
        
        public void Inserir(){
            Connection conn = DataBase.getConnection();
            try {
                PreparedStatement stmt = conn.prepareStatement("INSERT INTO pedido (sabor, tamanho, nomeC, pagamento, localEntr) VALUES (?,?,?,?,?)");
                stmt.setString(1, this.getSabor());
                stmt.setString(2, this.getTamanho());
                stmt.setString(3, this.getNomeCliente());
                stmt.setString(4, this.getPagamento());
                stmt.setString(5, this.getLocalEntr());
                
                stmt.execute();
            } catch (SQLException ex) {
                Logger.getLogger(Pedido.class.getName()).log(Level.SEVERE, null, ex);
            }
        }
        public void pedidoList(){
            System.out.println("Sabor: " + this.sabor
                + "Tamanho: " + this.tamanho
                + "Nome: " + this.nomeCliente
                + "Forma de pagamento: " + this.pagamento
                + "Local de entrega: " + this.localEntr);
        }
    }















