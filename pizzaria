    private void gtActionPerformed(java.awt.event.ActionEvent evt) {                                   
        // TODO add your handling code here:
        this.dispose();
    }                                  

    private void sendActionPerformed(java.awt.event.ActionEvent evt) {                                     
        // TODO add your handling code here:
        //JOptionPane.showMessageDialog(rootPane,tfCliente.getText());
        String tamanho = cbTamanho.getSelectedItem().toString();
        String sabor = cbSabor.getSelectedItem().toString();
        String clienteNome = tfCliente.getText();
        String pagamento = bgPagamento.getSelection().getActionCommand();               
        String localEntr = tfLocal.getText();
        
        boolean error = false;
        
        if(sabor.length() == 0){
            JOptionPane.showMessageDialog(rootPane, "Por favor, selecione o sabor");
            error = true; 
        }
        if(tamanho.length() == 0){
            JOptionPane.showMessageDialog(rootPane, "Por favor, selecione o tamanho");
            error = true; 
        }
        if(clienteNome.length() == 0){
            JOptionPane.showMessageDialog(rootPane, "Por favor, insira o nome do cliente");
            error = true; 
        }
        if(pagamento.length() == 0){
            JOptionPane.showMessageDialog(rootPane, "Por favor, selecione a forma de pagamento");
            error = true; 
        }
        if(localEntr.length() == 0){
            JOptionPane.showMessageDialog(rootPane, "Por favor, insira o local de entrega");
            error = true; 
        }       
        if(!error){
            Pedido pedido = new Pedido();
            pedido.setTamanho(tamanho);
            pedido.setSabor(sabor);
            pedido.setNomeCliente(clienteNome);
            pedido.setPagamento(pagamento);
            pedido.setLocalEntr(localEntr);
            int confirmar = JOptionPane.showConfirmDialog(rootPane, 
                "Sabor: " + sabor + 
                "\nTamanho:  " + tamanho+
                "\nNome cliente: " + clienteNome + 
                "\nPagamento: " + pagamento + 
                "\nLocal entrega: " + localEntr, 
                "Confirmar?",
                JOptionPane.YES_NO_OPTION);
            if(confirmar == JOptionPane.YES_OPTION){
                pedido.Inserir();
                JOptionPane.showMessageDialog(rootPane, "Pedido enviado");
                cbTamanho.setSelectedItem(null);
                cbSabor.setSelectedItem(null);
                tfCliente.setText("");
                bgPagamento.clearSelection();
                tfLocal.setText("");
            }
                    
         }
        
        
        //pedido.pedidoList();
        //JOptionPane.showInternalMessageDialog(rootPane, );
    }                                    

